{
  "imports": {
    "@std/": "jsr:@std/",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.2",
    "@types/react": "npm:@types/react@18.2.0/index.d.ts",
    "@types/react-dom": "npm:@types/react-dom@18.2.0/index.d.ts",
    "hono": "npm:hono@3.12.0",
    "react": "npm:react@18.2.0",
    "react-dom/client": "npm:react-dom@18.2.0/client",
    "react-dom/server": "npm:react-dom@18.2.0/server",
    "react/jsx-runtime": "npm:react@18.2.0/jsx-runtime"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": ["deno.ns", "dom", "dom.iterable", "esnext"],
    "types": ["react", "react-dom"]
  },
  "tasks": {
    "dev": "deno run --allow-net --allow-read --allow-env --allow-run --watch src/server.tsx",
    "start": "deno run --allow-net --allow-read src/server.tsx"
  }
}
